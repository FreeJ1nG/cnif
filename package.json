{
  "name": "cnif",
  "type": "module",
  "version": "1.0.1",
  "license": "ISC",
  "author": "FreeJ1nG",
  "description": "Utility library to do if conditionals, better used with clsx and twMerge",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json"
  },
  "keywords": [
    "clsx",
    "tailwind-merge",
    "utility",
    "css",
    "resolution"
  ],
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-rational": "^5.0.14",
    "typescript": "^5.4.5"
  }
}
